<div class="container">
    <main id="main">
    
        <section class="contact section-bg">
            
                <div class="section-title" data-aos="fade-up">
                    <h2><%= organization.name %></h2>
                </div>
                <% if(organization.object) { %>
                <div class="table-responsive" >
                    <table class="table ">
                        <thead>
                        <tr>
                            <th>Date</th>
                            <th>Object</th>
                            <th>Email</th>
                            <th>Address</th>
                            <th>Contact</th>
                        </tr>
                        </thead>
                        <tbody>
                           
                            <tr>
                                <td><%= organization.date %></td>
                                <td><%= organization.object %></td>
                                <td><%= organization.email %></td>
                                <td><%= organization.address %></td>
                                <td><%= organization.contact %></td>
                            <tr>                                    
                        </tbody>
                    </table>
                    <br>
                    <h3 style="text-align: center;">List of Proprietor(s)</h3>
                    <table class="table ">
                        <thead>
                        <tr>
                            <th>S/N</th>
                            <th>Name</th>
                            <th>Address</th>
                            <th>Email</th>
                            <th>D.O.B</th>
                            <th>Contact</th>
                        </tr>
                        </thead>
                        <tbody>
                            <% var i = 0 %>
                            <% organization.proprietors.forEach(function(proprietor) { %>
                            <% if(proprietor.name) { %>
                                <tr>
                                    <td><%= ++i %></td>
                                    <td><%= proprietor.name %></td>
                                    <td><%= proprietor.address %></td>
                                    <td><%= proprietor.email %></td>
                                    <td><%= proprietor.date %></td>
                                    <td><%= proprietor.contact %></td>
                                <tr>                                   
                                <% } %>
                            <% }) %>
                        </tbody>
                    </table>
                    <br>
                    <h3 style="text-align: center;">Payment</h3>
                    <table class="table ">
                        <thead>
                        <tr>
                            <th>S/N</th>
                            <th>Date</th>
                            <th>Amount</th>
                        </tr>
                        </thead>
                        <tbody>
                            <% var i = 0 %>
                            <% organization.payment.forEach(function(payment) { %>
                                <tr>
                                    <td><%= ++i %></td>
                                    <td><%= payment.date %></td>
                                    <td><%= payment.amount %></td>
                                <tr>                                   
                            <% }) %>
                        </tbody>
                    </table>
                </div> <br>
                <h3 style="text-align: center;">Attached Documents</h3>
                <div style="display:flex; flex-wrap: wrap;">
                    <% organization.identity.forEach(function(identity){%>
                    <div class="col-md-3 col-sm-6">
                        <div class="thumbnail">
                            <img src="<%= identity%>">
                        </div> 
                    </div>    
                    <% }); %>
                </div>
                <% } else { %>
                    
                    <div class="table-responsive" >
                        <table class="table ">
                            <thead>
                            <tr>
                                <th>Date</th>
                                <th>Nature</th>
                                <th>Address</th>
                                <th>Shares</th>
                            </tr>
                            </thead>
                            <tbody>
                               
                                <tr>
                                    <td><%= organization.date %></td>
                                    <td><%= organization.nature %></td>
                                    <td><%= organization.address %></td>
                                    <td><%= organization.shares %></td>
                                <tr>                                    
                            </tbody>
                        </table>
                        <br>
                        <h3 style="text-align: center;">List of Director(s)</h3>
                        <table class="table ">
                            <thead>
                            <tr>
                                <th>S/N</th>
                                <th>Name</th>
                                <th>Address</th>
                                <th>Email</th>
                                <th>D.O.B</th>
                                <th>Contact</th>
                                <th>Allotment</th>
                                <th>Nationality</th>
                                <th>Gender</th>
                            </tr>
                            </thead>
                            <tbody>
                                <% var i = 0 %>
                                <% organization.directors.forEach(function(director) { %>
                                <% if(director.name) { %>
                                    <tr>
                                        <td><%= ++i %></td>
                                        <td><%= director.name %></td>
                                        <td><%= director.address %></td>
                                        <td><%= director.email %></td>
                                        <td><%= director.date %></td>
                                        <td><%= director.contact %></td>
                                        <td><%= director.allotment %></td>
                                        <td><%= director.nationality %></td>
                                        <td><%= director.gender %></td>
                                    <tr>                                   
                                    <% } %>
                                <% }) %>
                            </tbody>
                        </table>
                        <br>

                        <h3 style="text-align: center;">Secretary's Information</h3>
                        <table class="table ">
                            <thead>
                            <tr>
                                <th>Name</th>
                                <th>Address</th>
                                <th>Email</th>
                                <th>D.O.B</th>
                                <th>Contact</th>
                                <th>Nationality</th>
                                <th>Gender</th>
                            </tr>
                            </thead>
                            <tbody>
                                <% if(organization.secretary.name) { %>
                                    <tr>
                                        <td><%= organization.secretary.name %></td>
                                        <td><%= organization.secretary.address %></td>
                                        <td><%= organization.secretary.email %></td>
                                        <td><%= organization.secretary.date %></td>
                                        <td><%= organization.secretary.contact %></td>
                                        <td><%= organization.secretary.nationality %></td>
                                        <td><%= organization.secretary.gender %></td>
                                    <tr>                                   
                                    <% } %>
                            </tbody>
                        </table>
                        <br>

                        <h3 style="text-align: center;">Payment</h3>
                        <table class="table ">
                            <thead>
                            <tr>
                                <th>S/N</th>
                                <th>Date</th>
                                <th>Amount</th>
                            </tr>
                            </thead>
                            <tbody>
                                <% var i = 0 %>
                                <% organization.payment.forEach(function(payment) { %>
                                    <tr>
                                        <td><%= ++i %></td>
                                        <td><%= payment.date %></td>
                                        <td><%= payment.amount %></td>
                                    <tr>                                   
                                <% }) %>
                            </tbody>
                        </table>
                    </div> <br>
                    <h3 style="text-align: center;">Attached Documents</h3>
                    <div style="display:flex; flex-wrap: wrap;">
                        <% organization.identity.forEach(function(identity){%>
                        <div class="col-md-3 col-sm-6">
                            <div class="thumbnail">
                                <img src="<%= identity%>">
                            </div> 
                        </div>    
                        <% }); %>
                    </div>

                <% } %>    
                


        </section><!-- End form Section -->
        <% if(currentUser.isAdmin) { %>
        <div class="container">
          <h4 class="pull-right"><a href="#"><input type="submit"  value="Make Payment" class="btn btn-primary"></a></h4>
            <p>
                <form action="/payment?q=<%= organization._id%>" method="POST" class="form-inline">
                    <div class="form-group">
                        <input type="text" name="amount" placeholder="enter payment amount..." class="form-control">
                        <input type="submit"  class="btn btn-primary">
                    </div>
                </form>
            </p>
        </div>
        <% } %>

    </main><!-- End #main -->
        

