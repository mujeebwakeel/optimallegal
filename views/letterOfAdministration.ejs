    <main class="app-content">
      <div class="app-title">
        <div>
          <h1><i class="fa fa-edit"></i> Letter of Administration</h1>
            <p> Fill the information below appropriately</p>
        </div>
        <ul class="app-breadcrumb breadcrumb">
          <li class="breadcrumb-item"><i class="fa fa-home fa-lg"></i></li>
          <li class="breadcrumb-item">Optimal Legalpreneurs Dashboard</li>
          <li class="breadcrumb-item"><a href="#"> Letter of Administration</a></li>
        </ul>
      </div>
      <div class="container-fluid">
        <% if(message && message.length > 0){ %>
        <div class="alert alert-danger" role="alert">
              <p><%= message %></p>
        </div>
        <% } %>
        <% if(logout && logout.length > 0) { %>
        <div class="alert alert-warning" role="alert">
              <p><%= logout %>. Thank you for visiting our site</p>
        </div>
        <% } %>
        <% if(error && error.length > 0){ %>
        <div class="alert alert-danger" role="alert">
              <p><%= error %></p>
        </div>
        <% } %>
        <% if(success && success.length > 0){ %>
        <div class="alert alert-success" role="alert">
              <p><%= success %></p>
        </div>
        <% } %>
      </div>
      <div class="row">
        <div class="col-md-12">
          <div class="tile">
            <div class="row">
              <div class="col-lg-6">
                <form id="letter" action="/letter" method="POST">
                  <div class="form-group">
                    <label for="Name">What is your Name?</label>
                    <input class="form-control" id="Name" type="text"  placeholder="Enter your name" required="" name="letter[name]">
                  </div>
                  <div class="form-group">
                    <label for="address">What is your home address</label>
                    <input class="form-control" id="address" type="text" name="letter[address]"
                           placeholder="e.g. layout 5, Bernard street,bodija" required="">
                  </div>
                  <div class="form-group">
                    <label for="PhoneNo"> What is your contact</label>
                      <input class="form-control" id="PhoneNo" type="tel" name="letter[phone]"
                             required="">
                  </div>
                  <div class="form-group">
                      <div class="form-group">
                          <label for="e-mail"> Enter your E-mail</label>
                          <input class="form-control-file" id="e-mail" type="email" name="letter[email]"
                                 placeholder="e.g.smith@gmail.com" required="">
                      </div>
                    <label for="help">How may we help you?</label>
                    <textarea class="form-control" id="help" rows="3" required="How may we help you, please" name="letter[content]"></textarea>
                </div>
                <div class="form-group">
                    <label >Enter your signature</label>
                    <div id="signature"></div>
                    <button type="button" class="btn btn-danger btn-sm" id="btnClear" >Clear</button>
                    <button type="button" class="btn btn-primary btn-sm" id="btnSave">Save</button>
                    <input type="hidden" id="hiddenSigData" name="signature" />
                </div>
                  <div>Note that the price of this service is 10% of the estate</div>
                  <button class="btn btn-primary" id="submit" disabled>Submit</button>
                </form> 
              </div>
            </div>
            <div class="tile-footer">

            </div>
          </div>
        </div>
      </div>
