      <div class="container-fluid">
        <% if(message && message.length > 0){ %>
        <div class="alert alert-danger" role="alert">
              <p><%= message %></p>
        </div>
        <% } %>
        <% if(logout && logout.length > 0) { %>
        <div class="alert alert-warning" role="alert">
              <p><%= logout %>. Thank you for visiting our site</p>
        </div>
        <% } %>
        <% if(error && error.length > 0){ %>
        <div class="alert alert-danger" role="alert">
              <p><%= error %></p>
        </div>
        <% } %>
        <% if(success && success.length > 0){ %>
        <div class="alert alert-success" role="alert">
              <p><%= success %></p>
        </div>
        <% } %>
      </div>
        <form id="company" action="/company" method="POST" enctype="multipart/form-data">
      <div class="row">
        <div class="col-md-12">
          <div class="tile">
            <div class="row">
                <form>
                 <div class="col-lg-12">
                  <div class="form-group">
                      <h2>Fill in your Company details below</h2>
                    <label for="businessname">Choose a company's name</label>
                    <input class="form-control" id="businessname" type="text" aria-describedby="emailHelp" name="company[name]"
                           placeholder="Choose a company's name"><small class="form-text text-muted" id="emailHelp"></small>
                  </div>
                  <div class="form-group" style="display: none;">
                    <label for="ref">Payment Reference</label>
                    <input class="form-control" id="ref" type="text"  placeholder="Enter your name" required="" name="company[paymentref]">
                  </div>
                  <div class="form-group">
                    <label for="objective"> What is the nature of your company</label>
                    <input class="form-control" id="objective" type="text" placeholder="e.g. real-estate" required="" name="company[nature]">
                  </div>
                    <div class="form-group">
                    <label for="address"> Where is the address of your company?</label>
                      <input class="form-control" id="address" type="text"
                             placeholder="e.g. suite 20,lion plaza,bernard street,bodja" required="" name="company[address]">
                  </div>
                     <div class="form-group">
                         <label for="objective"> Total Share Caital</label>
                         <input class="form-control" id="objective" type="text" placeholder="Enter share capital amount" required="" name="company[share]">
                     </div>
                  </div>
                 </div>
        </div>
      </div>
    </main>


    <!--directors and appointment of company registration begin-->

    <main class="app-content">
        <div class="app-title">
            <div>
                <h1><i class="fa fa-edit"></i> Directors and Secretary INFO</h1>
                <p>Provide the information below appropriately</p>
            </div>
            <ul class="app-breadcrumb breadcrumb">
                <li class="breadcrumb-item" href="/dashboard"><i class="fa fa-home fa-lg"></i></li>
                <li class="breadcrumb-item">Corporate Registration</li>
                <li class="breadcrumb-item"><a href="#">Company's Name</a></li>
            </ul>
        </div>
        <form>
            <div class="row">
                <div class="col-md-12">
                    <div class="tile">
                        <div class="row">
                            <form>
                                <div class="col-lg-6">
                                    <h2>Director's Info</h2>
                                    <h6>Fill in your details below</h6>
                                <div class="form-group">
                                        <label for="businessname">Director(s) Name</label>
                                        <input class="form-control" id="businessname" type="text" aria-describedby="emailHelp" name="director[name]"
                                               placeholder="e.g. chris"><small class="form-text text-muted" id="emailHelp"></small>
                                    </div>
                                    <div class="form-group">
                                        <label for="gender" >Select Gender </label>
                                        <select class=" form-control" id="gender" name="director[gender]">
                                            <option>Gender</option>
                                            <option value="male">Male</option>
                                            <option value="female">Female</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label for="address"> Nationality </label>
                                        <input class="form-control" id="address" type="text" name="director[nationality]"
                                               placeholder="e.g. Nigerian" required="">
                                    </div>
                                    <div class="form-group">
                                        <label for="address"> Where is the address of your company's director ?</label>
                                        <input class="form-control" id="address" type="text"
                                               placeholder="e.g. suite 20,lion plaza,bernard street,bodja" required="" name="director[address]">
                                    </div>
                                    <div class="form-group">
                                        <label for="address"> Date of Birth </label>
                                        <input class="form-control" id="address" type="text" name="director[date]"
                                               placeholder="e.g. 09/12/1900" required="">
                                    </div>
                                    <div class="form-group">
                                        <label for="e-mail">E-mail</label>
                                        <input class="form-control" type="email" id="e-mail" placeholder="e.g queens@gmail.com" required="" name="director[email]">
                                    </div>
                                    <div class="form-group">
                                        <label for="phoneNo">Enter your phone number</label>
                                        <input class="form-control" type="tel" id="phoneNo" required="" name="director[phone]" >
                                    </div>
                                    <div class="form-group">
                                        <label for="valid id" >Select ID </label>
                                        <select class=" form-control" id="valid id" name="director[idType]">
                                            <option>Select</option>
                                            <option value="National ID">National ID</option>
                                            <option value="International Passport">International Passport</option>
                                            <option value="Driver's License">Driver's License</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">Upload ID</label>
                                        <input class="form-control" type="file" accept="image/*" name="image">
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">Upload Passport Photograph</label>
                                        <input class="form-control" type="file" accept="image/*" name="image">
                                    </div>
                                    </div>

                                <div class="col-lg-6">
                                    <h2>Appointment of Company Secretary</h2>
                                    <h6>Fill in your secretary's details below</h6>
                                    <div class="form-group">
                                        <label for="businessname">Secretary Name</label>
                                        <input class="form-control" id="businessname" type="text" aria-describedby="emailHelp" name="secretary[name]"
                                               placeholder="e.g. chris"><small class="form-text text-muted" id="emailHelp"></small>
                                    </div>
                                    <div class="form-group">
                                        <label for="gender" >Select Gender </label>
                                        <select class=" form-control" id="gender" name="secretary[gender]">
                                            <option>Gender</option>
                                            <option value="male">Male</option>
                                            <option value="female">Female</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label for="address"> Nationality </label>
                                        <input class="form-control" id="address" type="text" name="secretary[nationality]"
                                               placeholder="e.g. Nigerian" required="">
                                    </div>
                                    <div class="form-group">
                                        <label for="address"> Where is the address of your company's secretary ?</label>
                                        <input class="form-control" id="address" type="text" name="secretary[address]"
                                               placeholder="e.g. suite 20,lion plaza,bernard street,bodja" required="">
                                    </div>
                                    <div class="form-group">
                                        <label for="address"> Date of Birth </label>
                                        <input class="form-control" id="address" type="text" name="secretary[date]"
                                               placeholder="e.g. 09/12/1900" required="">
                                    </div>
                                    <div class="form-group">
                                        <label for="e-mail">E-mail</label>
                                        <input class="form-control" type="email" id="e-mail" placeholder="e.g queens@gmail.com" name="secretary[email]" required="">
                                    </div>
                                    <div class="form-group">
                                        <label for="phoneNo">Enter secretary's phone number</label>
                                        <input class="form-control" type="tel" id="phoneNo" required=""  name="secretary[phone]">
                                    </div>
                                    <div class="form-group">
                                        <label for="valid id" >Select ID </label>
                                        <select class=" form-control" id="valid id" name="secretary[idType]">
                                            <option>Select</option>
                                            <option value="National ID">National ID</option>
                                            <option value="International Passport">International Passport</option>
                                            <option value="Driver's License">Driver's License</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">Upload ID</label>
                                        <input class="form-control" type="file" accept="image/*" name="image">
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">Upload Passport Photograph</label>
                                        <input class="form-control" type="file" accept="image/*" name="image">
                                    </div>
                                </div>
                                <script src="https://js.paystack.co/v1/inline.js"></script>
                                    <button class="btn btn-primary" type="button" onclick="payWithPaystack()">Submit</button>

                        </div>
                    </div>
                </div>
            </div>
    </main>

